<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f1f3f8"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="405dp"
        android:layout_height="86dp"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="625dp"
        android:background="#ffa0d2"
        android:focusable="auto"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="429dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:contentDescription="@string/this_our_logo_for_app"
            app:srcCompat="@drawable/logoapp" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="488dp"
            android:layout_height="51dp"
            android:layout_marginTop="23sp"
            android:layout_weight="1"
            android:fontFamily="@font/berkshire_swash"
            android:shadowColor="#494646"
            android:text="@string/the_dating_app"
            android:textColor="#494646"
            android:textSize="27sp"
            android:textStyle="normal" />
    </LinearLayout>

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="410dp"
        android:layout_height="630dp"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="130sp"
                android:layout_marginLeft="130sp"
                android:layout_marginTop="15sp"
                android:fontFamily="@font/belgrano"
                android:text="@string/admin_panal"
                android:textColor="#494646"
                android:textSize="30sp"
                android:textStyle="normal" />

        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="162dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="450dp"
        android:background="#f1f3f8"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.cardview.widget.CardView
            android:layout_width="164dp"
            android:layout_height="95dp"
            android:layout_marginStart="18sp"
            android:layout_marginLeft="18sp"
            android:layout_marginTop="10sp"
            android:layout_marginEnd="25sp"
            android:layout_marginRight="5sp"
            android:background="#FFF171"
            app:cardBackgroundColor="#ffa0d2"
            app:cardCornerRadius="15dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:contentDescription="@string/newuser"
                    android:padding="5sp"
                    app:srcCompat="@drawable/rasa1" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="17sp"
                        android:fontFamily="@font/belgrano"
                        android:text="@string/textview7"
                        android:textColor="#494646"
                        android:textSize="15sp"
                        android:textStyle="normal"
                        tools:text="@string/textview7" />

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15sp"
                        android:fontFamily="@font/belgrano"
                        android:text="@string/textview8"
                        android:textColor="#494646"
                        android:textSize="19sp"
                        tools:text="@string/textview8" />

                </LinearLayout>
            </LinearLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="161dp"
            android:layout_height="93dp"
            android:layout_marginStart="18sp"
            android:layout_marginLeft="18sp"
            android:layout_marginTop="10sp"
            android:background="#FFF171"
            app:cardBackgroundColor="#ffa0d2"
            app:cardCornerRadius="15dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:contentDescription="@string/registered_users"
                    android:padding="4sp"
                    app:srcCompat="@drawable/rasa2" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="17sp"
                        android:fontFamily="@font/belgrano"
                        android:text="@string/textview9"
                        android:textColor="#494646"
                        android:textSize="17sp"
                        android:textStyle="normal"
                        tools:text="@string/textview9" />

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15sp"
                        android:fontFamily="@font/belgrano"
                        android:text="@string/textview2"
                        android:textColor="#494646"
                        android:textSize="19sp"
                        tools:text="@string/textview2" />
                </LinearLayout>
            </LinearLayout>
        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <ScrollView
        android:layout_width="414dp"
        android:layout_height="448dp"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/scrollView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="380dp"
                android:layout_height="118dp"
                android:layout_marginStart="15sp"
                android:layout_marginLeft="15sp"
                app:cardBackgroundColor="#ffa0d2"
                app:cardCornerRadius="15dp">

                <Button
                    android:id="@+id/button7"
                    android:layout_width="290dp"
                    android:layout_height="85sp"
                    android:layout_marginStart="50sp"
                    android:layout_marginLeft="50sp"
                    android:layout_marginTop="18sp"
                    android:background="@drawable/button_back"
                    android:fontFamily="@font/belgrano"
                    android:text="@string/manage_users"
                    android:textColor="#494646"
                    android:textSize="20sp" />
            </androidx.cardview.widget.CardView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="153dp"
                android:layout_marginTop="10sp"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:layout_width="178dp"
                    android:layout_height="118dp"
                    android:layout_marginStart="15sp"
                    android:layout_marginLeft="15sp"
                    android:layout_marginTop="15sp"
                    app:cardBackgroundColor="#ffa0d2"
                    app:cardCornerRadius="15dp">

                    <LinearLayout
                        android:layout_width="176dp"
                        android:layout_height="121dp"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageView4"
                            android:layout_width="143dp"
                            android:layout_height="72dp"
                            android:layout_marginStart="20sp"
                            android:layout_marginLeft="20sp"
                            android:layout_marginTop="4sp"
                            android:contentDescription="@string/barchart"
                            app:srcCompat="@drawable/rasa3" />

                        <Button
                            android:id="@+id/button4"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:background="@drawable/button_back"
                            android:fontFamily="@font/belgrano"
                            android:text="@string/area_popularity"
                            android:textColor="#494646" />

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="189dp"
                    android:layout_height="118dp"
                    android:layout_marginStart="12sp"
                    android:layout_marginLeft="12sp"
                    android:layout_marginTop="15sp"
                    app:cardBackgroundColor="#ffa0d2"
                    app:cardCornerRadius="15dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageView5"
                            android:layout_width="145dp"
                            android:layout_height="73dp"
                            android:layout_marginStart="20sp"
                            android:layout_marginLeft="20sp"
                            android:layout_marginTop="3sp"
                            android:contentDescription="@string/piechart"
                            app:srcCompat="@drawable/rasa4" />

                        <Button
                            android:id="@+id/button3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/button_back"
                            android:fontFamily="@font/belgrano"
                            android:text="@string/gender_popularity"
                            android:textColor="#494646" />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <androidx.cardview.widget.CardView
                android:layout_width="380dp"
                android:layout_height="330dp"
                android:layout_marginStart="15sp"
                android:layout_marginLeft="15sp"
                android:layout_marginEnd="18sp"
                android:layout_marginRight="18sp"
                android:layout_marginBottom="15sp"
                app:cardBackgroundColor="#ffa0d2"
                app:cardCornerRadius="15dp">

                <CalendarView
                    android:id="@+id/calendarView4"
                    android:layout_width="366dp"
                    android:layout_height="310dp"
                    android:layout_marginStart="8sp"
                    android:layout_marginLeft="8sp"
                    android:layout_marginTop="10sp" />
            </androidx.cardview.widget.CardView>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>